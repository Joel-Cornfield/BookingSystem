{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    "Token": "YOUR_JWT_SECRET_KEY_HERE",
    "Issuer": "YOUR_JWT_ISSUER_HERE",
    "Audience": "YOUR_JWT_AUDIENCE_HERE"
  },
  "Cloudinary": {
    "Url": "YOUR_CLOUDINARY_URL_HERE"
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_DATABASE_CONNECTION_STRING_HERE"
  }
}
